{
  "name": "dognut",
  "version": "1.0.0",
  "description": "DOM renderer with no XML-like expression",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "Chanjin Park <neocjmix@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neocjmix/dognut.git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^7.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "js-combinatorics": "^0.5.5",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "^10.2.0",
    "sinon": "^9.0.1",
    "ts-mocha": "^7.0.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:package": "tsc -p tsconfig.package.json"
  },
  "mocha": {
    "require": [
      "@babel/register",
      "ts-mocha"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "to-style": "^1.3.3"
  }
}
